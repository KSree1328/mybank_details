        <HBox id="hBox3">
            <Label id="lblIFSCId" text="{i18n>ifscode}" class="sapUiSmallMarginEnd"/>
            <Text id="idIFSCValue" text="SBIN002345"/>
        </HBox>
        
    </VBox>


</core:FragmentDefinition>