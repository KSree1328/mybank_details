<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">

<Dialog id="moreBankDetails" title="Confidential Details">

<VBox id="vBox1" class="sapUiSmallMargin">
        <!-- <Title text="Account Details" id="accDetails" level="H1"/> -->

        <HBox id="hBox1">
            <Label id="bankAccNum" text="{i18n>accNum}" class="sapUiSmallMarginEnd"/>
            <Text id="accountNum" text="3456789"/>
        </HBox>

        <HBox id="hBox2">
            <Label id="lblAcHolderNameID" text="{i18n>accName}" class="sapUiSmallMarginEnd"/>
            <Text id="idAcHolderNameValue" text="KSree"/>
        </HBox>

        <HBox id="hBox3">
            <Label id="lblIFSCId" text="{i18n>isfcode}" class="sapUiSmallMarginEnd"/>
            <Text id="idIFSCValue" text="SBIN002345"/>
        </HBox>

        <HBox id="hBox4">
            <Label id="lblCustId" text="{i18n>custID}" class="sapUiSmallMarginEnd"/>
            <Text id="idCustValue" text="1234*****"/>
        </HBox>

        <HBox id="hBox5">
            <Label id="lblAddressId" text="{i18n>add}" class="sapUiSmallMarginEnd"/>
            <Text id="idAddressValue" text="***********"/>
        </HBox>
        
    </VBox>


    <beginButton>
    <Button id="closeBtn" text="Close" press=".onCloseBankDetails"></Button>
    </beginButton>
</Dialog>

</core:FragmentDefinition>